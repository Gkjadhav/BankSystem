extends base.pug

block style 
    style 
        include ../static/style_base.css
        include ../static/style_members.css

block content 
    div.container 
        h1 Members List 
        table 
            tr
                th Name
                th Account No
                th Email
                th Balance

            each user in users  
                tr                 
                    td #{user.name}
                    td #{user.accountNo}
                    td #{user.email}
                    td #{user.balance}
            
        table

   

    